/* pages/login.wxss */
.login-container{
  width: 750rpx;
  height: 100%;
  background: #1d1d28;
  position: relative;
}
.login-logo{
  width: 100%;
  height: auto;
  text-align: center;
} 
.login-logo image{
  width: 250rpx;
  height: 126rpx;
  margin: 60rpx auto;
}
.tab-header{
  width: 100%;
  height: 108rpx;
  display: -webkit-flex;
  color: #9DA3B4;
  font-size: 30rpx;
  
}
.tab-header view{
  width: 50%;
  height: 100%;
  text-align: center;
  border-right: 1px dotted #484855;
  position: relative;
  background-image:url(data:image/png;base64,);
  background-position: top center;
  background-repeat: no-repeat;
  background-size: 48rpx 48rpx;
}
.tab-header text{
  position: absolute;
  bottom: 0;
  width: 100%;
  left: 0;
  text-align: center;
}
.verif-button{
  border:none!important;
  background-image:url(data:image/png;base64,)!important;
  background-position: top center;
  background-repeat: no-repeat;
  background-size: 48rpx 48rpx;
}
.swiper-container{
  width: 100%;
  height: 694rpx;
  background: #fff;
  border-radius:20rpx 20rpx 0 0;
  position: absolute;
  bottom: 0;
  left: 0;
}
.swiper-container form{
  width: 608rpx;
  height: auto;
  display: block;
  margin: 0 auto;
}
.swiper-container form view{
  width: 100%;
  display: -webkit-flex;
  align-items: center;
  border-bottom:1rpx solid #E5E5E5;
  margin-top: 60rpx;
  padding-bottom: 20rpx;
}
.phone-wrapper input,.verif-wrapper input{
  font-size: 30rpx;
  color: #333;
}
.login-button{
  width: 608rpx;
  height: 88rpx;
  display: block;
  text-align: center;
  color: #fff;
  border:none;
  outline: none;
  font-size: 34rpx;
  border-radius: 200rpx;
  margin-top: 60rpx;
}
.other-button{
  width: 608rpx;
  display: -webkit-flex;
  justify-content: space-between;
  margin: 40rpx auto 0;
}
.other-button navigator{
  color: #9DA3B4;
  font-size: 30rpx;
}